    "import tensorflow as tf\n",
    "\n",
    "# Check if a GPU is available\n",
    "gpus = tf.config.list_physical_devices('GPU')\n",
    "if gpus:\n",
    "    print(f\"GPUs detected: {len(gpus)}\")\n",
    "    for gpu in gpus:\n",
    "        print(f\" - {gpu.name}\")\n",
    "else:\n",
    "    print(\"No GPUs detected.\")\n"
